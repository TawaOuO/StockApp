<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stock">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="stock"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".End"
                  android:screenOrientation="portrait"/>
        <activity android:name=".PickActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".PickingActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".TecPickingActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".BasicPickingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ChipPickingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
                  android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

<!--    <uses-permission android:name="android.permission.INTERNET" />-->
<!--    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />-->

</manifest>